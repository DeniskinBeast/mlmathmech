    "def live_stat(df: pd.DataFrame) -> (float, float):\n",
    "    woman_survived = df[df['Sex'] == 'female']['Survived'].mean()\n",
    "    men_survived = df[df['Sex'] == 'male'][\"Survived\"].mean()\n",
    "    return (woman_survived, men_survived)\n",
    "\n",
    "live_stat(data)"
